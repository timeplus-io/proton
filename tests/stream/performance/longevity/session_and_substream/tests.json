            "steps":[
                {"statements": [
                    {"client":"python","config_set": "setting_1","workers": 2, "interval":1, "query_type": "table", "query":"drop view if exists ttp_session_substream_v$"},                    
                    {"client":"python","config_set": "setting_1","workers": 2, "interval":1, "query_type": "table", "query":"drop stream if exists ttp_$"},
                    {"client":"python","config_set": "setting_1","workers": 2, "interval":1, "query_type": "table", "query":"CREATE STREAM if not exists ttp_$(`lpn` string, `vno` string, `drc` string, `drcCode` int32, `wgs84Lat` float32, `wgs84Lon` float32, `gcj02Lat` float32, `gcj02Lon` float32, `province` nullable(string), `city` nullable(string), `country` nullable(string), `spd` float32, `mil` float32, `time` string, `adr` string, perf_event_time datetime64(3) default now64(3,'Asia/Shanghai'), _perf_row_id string, _perf_ingest_time string, `_tp_time` datetime64(3, 'Asia/Shanghai') DEFAULT now64(3,'Asia/Shanghai') CODEC(DoubleDelta, LZ4), `_tp_index_time` datetime64(3, 'UTC') CODEC(DoubleDelta, LZ4)) TTL to_datetime(_tp_time) + INTERVAL 24 HOUR SETTINGS logstore_retention_bytes = '21474836480', logstore_retention_ms = '00000'"},                    
                    {"client":"python","config_set": "setting_1", "wait":20, "interval":2, "workers": 2, "query_type": "table", "query":"create view if not exists ttp_session_substream_v$ as (WITH duration AS(SELECT lpn, window_start AS ds, window_end AS de, date_diff('second', ds, de) AS du FROM session(ttp_$, _tp_time, 1s, 40s, spd > 0, spd = 0, lpn) GROUP BY lpn, window_start, window_end  SETTINGS seek_to = 'earliest') SELECT lpn, window_start, window_end, sum(du) FROM hop(duration, de, 10s, 40s) PARTITION BY lpn GROUP BY lpn, window_start, window_end HAVING  sum(du) > 1s)"},
                    {"client":"python","config_set": "setting_1", "wait": 10, "interval":1, "depends_on_stream":"ttp_session_substream_v$", "query_id":201, "run_mode":"process", "loop_times": 1, "workers": 1, "query_type": "stream", "terminate":"auto","query_end_timer": 30, "query":"select * from ttp_session_substream_v$", "result_keep":"False", "query_result_table":"query_result_03", "query_record_table":"query_record_01"},
                    {"client":"python", "config_set": "setting_1", "wait": 10, "query_id": 202, "run_mode":"process", "loop_times": 1, "workers": 1, "query_type": "stream", "terminate":"auto", "query_end_timer": 30, "query":"select * from ttp_$ where spd >0", "result_keep":"False", "query_result_table":"query_result_03", "query_record_table":"query_record_01"},
                    {"client":"python", "config_set": "setting_1", "wait": 10, "query_id": 203, "run_mode":"process", "loop_times": 1, "workers": 1, "query_type": "stream", "terminate":"auto", "query_end_timer": 30, "query":"select count_distinct(lpn) FROM (SELECT lpn, max(_tp_time) as lastSeen FROM ttp_$ group by lpn having lastSeen>date_sub(now(),10s) emit periodic 5s settings seek_to='-24h')", "result_keep":"False", "query_result_table":"query_result_03", "query_record_table":"query_record_01"},
                    {"client":"python","config_set": "setting_1","workers": 1,"query_id": 204, "interval":1, "loop_times": -1, "query_type": "table", "query":"select * from table(ttp_$) where spd >0"}

